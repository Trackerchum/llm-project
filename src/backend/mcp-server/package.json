	"devDependencies": {
		"@types/core-js": "^2.5.8",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.2.0",
		"concurrently": "^9.2.1",
